{
  "manifest_version":  2,
  "name": "Keyword Highlighter",
  "version": "1.0",
  "description": "This extension highlights selected keywords on a webpage",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}